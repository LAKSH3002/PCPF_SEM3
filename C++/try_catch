
#include <iostream>
using namespace std;

int main()
{
    try {
  int age = 15;
  if (age >= 18) {
    cout << "Access granted - you are old enough.";
  } else {
    throw 505;
  }
}
catch (...) {
  cout << "Access denied - You must be at least 18 years old.\n";
  
  try{
      int a = 10;
      int b = 0;
      int c=a/b;
      throw c;
  }
  catch(...)
  {
      cout<<"EXCEPTION"<<endl;
  }}}
  
