// Online C++ compiler to run C++ program online
#include <iostream>
class calculator{
    public:
    void calc(int a,int b)
    {
        std::cout<<a+b<<std::endl;
    }
    void calc(double a,int b)
    {
        std::cout<<a*b<<std::endl;
    }
    void calc(double a,double b)
    {
        std::cout<<a-b<<std::endl;
    }
    void calc(int a,double b)
    {
        std::cout<<a/b<<std::endl;
    }
};
int main() {
    
    std::cout << "Hello world!";
    calculator c;
    std::cout<<std::endl;
    c.calc(5,4);
    c.calc(5.5,6);
    c.calc(3.3,3.2);
    c.calc(8,2.0);
    return 0;
}
