#include<iostream>
using namespace std;
// method overriding in c++
class A{
    public:
    void display()
    {
        std::cout<<"WE ARE IN CLASS A"<<std::endl;
    }
    void calc()
    {
        int a=5678;int b=25523;
        std::cout<<a+b<<std::endl;
    }
};
class B:public A{
    public:
    // overriden method ib C++
    void display()
    {
        std::cout<<"WE ARE IN CLASS B"<<std::endl;
    }
    void calc()
    {
        int a = 69676;int b = 63646;
        std::cout<<a-b<<std::endl;
    }
};
int main()
{
    A obj1;
    obj1.display();
    
    B obj2;
    obj2.display();
    obj2.calc();
    
    return 0;
}
